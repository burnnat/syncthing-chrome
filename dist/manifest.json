{
	"name": "Syncthing Filesystem",
	"description": "A ChromeOS filesystem to browse files shared via Syncthing.",
	"version": "0.0.1",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": ["js/background.js"]
		}
	},
	"icons": {
		"16": "assets/icon-16.png",
		"32": "assets/icon-32.png",
		"128": "assets/icon-128.png"
	},
	"sockets": {
		"tcp": {
			"connect": "*"
		}
	}
}
