{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "noImplicitAny": false,
        "sourceMap": false,
        "rootDir": "src",
        "outDir": "dist/js",
        "baseUrl": "src",
        "paths": {
            "*": [
                "../type-overrides/*",
                "*"
            ],
            "app/*": ["app/*"],
            "common/*": ["common/*"],
            "lib/*": ["lib/*"]
        },
        "noEmitOnError": true
    }
}
